library(openxlsx)

#Females 
Females_Sample_Info <- read.xlsx("/Users/juliamouat/Library/Mobile Documents/com~apple~CloudDocs/LaSalle Lab/Down Syndrome/DMRichR/sample_info_all.xlsx", sheet = "Females")
Females_Sample_Info_Clean <- Females_Sample_Info[1:54,]
Females <- lm(percent_cpg_meth ~ MajorHeartDefect, data = Females_Sample_Info_Clean)
summary(Females)
Females <- lm(percent_cpg_meth ~ CD8T, data = Females_Sample_Info_Clean)
summary(Females)
Females <- lm(percent_cpg_meth ~ CD4T, data = Females_Sample_Info_Clean)
summary(Females)
Females <- lm(percent_cpg_meth ~ NK, data = Females_Sample_Info_Clean)
summary(Females)
Females <- lm(percent_cpg_meth ~ Bcell, data = Females_Sample_Info_Clean)
summary(Females)
Females <- lm(percent_cpg_meth ~ Monocytes, data = Females_Sample_Info_Clean)
summary(Females)
Females <- lm(percent_cpg_meth ~ Granulocytes, data = Females_Sample_Info_Clean)
summary(Females)
Females <- lm(percent_cpg_meth ~ nRBC, data = Females_Sample_Info_Clean)
summary(Females)
Females <- lm(percent_cpg_meth ~ Birthweight, data = Females_Sample_Info_Clean)
summary(Females)
Females <- lm(percent_cpg_meth ~ GestationalAge, data = Females_Sample_Info_Clean)
summary(Females)
Females <- lm(percent_cpg_meth ~ AgeBloodCollection, data = Females_Sample_Info_Clean)
summary(Females)
Females <- lm(percent_cpg_meth ~ Race, data = Females_Sample_Info_Clean)
summary(Females)
Females <- lm(percent_cpg_meth ~ EthnicityHispanic, data = Females_Sample_Info_Clean)
summary(Females)

Females <- lm(percent_cpg_meth ~ MajorHeartDefect + nRBC, data = Females_Sample_Info_Clean)
summary(Females)
Females <- lm(percent_cpg_meth ~ CD8T + nRBC, data = Females_Sample_Info_Clean)
summary(Females)
Females <- lm(percent_cpg_meth ~ CD4T + nRBC, data = Females_Sample_Info_Clean)
summary(Females)
Females <- lm(percent_cpg_meth ~ NK + nRBC, data = Females_Sample_Info_Clean)
summary(Females)
Females <- lm(percent_cpg_meth ~ Bcell + nRBC, data = Females_Sample_Info_Clean)
summary(Females)
Females <- lm(percent_cpg_meth ~ Monocytes + nRBC, data = Females_Sample_Info_Clean)
summary(Females)
Females <- lm(percent_cpg_meth ~ Granulocytes + nRBC, data = Females_Sample_Info_Clean)
summary(Females)
Females <- lm(percent_cpg_meth ~ Birthweight + nRBC, data = Females_Sample_Info_Clean)
summary(Females)
Females <- lm(percent_cpg_meth ~ GestationalAge + nRBC, data = Females_Sample_Info_Clean)
summary(Females)
Females <- lm(percent_cpg_meth ~ AgeBloodCollection + nRBC, data = Females_Sample_Info_Clean)
summary(Females)
Females <- lm(percent_cpg_meth ~ Race + nRBC, data = Females_Sample_Info_Clean)
summary(Females)
Females <- lm(percent_cpg_meth ~ EthnicityHispanic + nRBC, data = Females_Sample_Info_Clean)
summary(Females)



#Males
Males_Sample_Info <- read.xlsx("/Users/juliamouat/Library/Mobile Documents/com~apple~CloudDocs/LaSalle Lab/Down Syndrome/DMRichR/sample_info_all.xlsx", sheet = "Males")
