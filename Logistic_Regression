summary(Males)
Males <- glm(MajorHeartDefect ~ Monocytes, family = binomial(link=logit), data = Males_Sample_Info_Clean)
summary(Males)
Males <- glm(MajorHeartDefect ~ Granulocytes, family = binomial(link=logit), data = Males_Sample_Info_Clean)
summary(Males)
Males <- glm(MajorHeartDefect ~ nRBC, family = binomial(link=logit), data = Males_Sample_Info_Clean)
summary(Males)
Males <- glm(MajorHeartDefect ~ Birthweight, family = binomial(link=logit), data = Males_Sample_Info_Clean)
summary(Males)
Males <- glm(MajorHeartDefect ~ GestationalAge, family = binomial(link=logit), data = Males_Sample_Info_Clean)
