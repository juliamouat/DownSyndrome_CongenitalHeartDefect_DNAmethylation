library(openxlsx)

#Females
Females_Sample_Info <- read.xlsx("/Users/juliamouat/Library/Mobile Documents/com~apple~CloudDocs/LaSalle Lab/Down Syndrome/DMRichR/sample_info_all.xlsx", sheet = "Females")
Females_Sample_Info_Clean <- Females_Sample_Info[1:54,]
Females <- glm(MajorHeartDefect ~ percent_cpg_meth, family = binomial(link = logit), data = Females_Sample_Info_Clean)
summary(Females)
Females <- glm(MajorHeartDefect ~ CD8T, family = binomial(link = logit), data = Females_Sample_Info_Clean)
summary(Females)
Females <- glm(MajorHeartDefect ~ CD4T, family = binomial(link=logit), data = Females_Sample_Info_Clean)
summary(Females)
Females <- glm(MajorHeartDefect ~ NK, family = binomial(link=logit), data = Females_Sample_Info_Clean)
summary(Females)
Females <- glm(MajorHeartDefect ~ Bcell, family = binomial(link=logit), data = Females_Sample_Info_Clean)
summary(Females)
Females <- glm(MajorHeartDefect ~ Monocytes, family = binomial(link=logit), data = Females_Sample_Info_Clean)
summary(Females)
Females <- glm(MajorHeartDefect ~ Granulocytes, family = binomial(link=logit), data = Females_Sample_Info_Clean)
summary(Females)
Females <- glm(MajorHeartDefect ~ nRBC, family = binomial(link=logit), data = Females_Sample_Info_Clean)
summary(Females)
Females <- glm(MajorHeartDefect ~ Birthweight, family = binomial(link=logit), data = Females_Sample_Info_Clean)
summary(Females)
Females <- glm(MajorHeartDefect ~ GestationalAge, family = binomial(link=logit), data = Females_Sample_Info_Clean)
summary(Females)
Females <- glm(MajorHeartDefect ~ AgeBloodCollection, family = binomial(link=logit), data = Females_Sample_Info_Clean)
summary(Females)
Females <- glm(MajorHeartDefect ~ Race, family = binomial(link=logit), data = Females_Sample_Info_Clean)
summary(Females)
Females <- glm(MajorHeartDefect ~ EthnicityHispanic, family = binomial(link=logit), data = Females_Sample_Info_Clean)
summary(Females)

#Males
Males_Sample_Info <- read.xlsx("/Users/juliamouat/Library/Mobile Documents/com~apple~CloudDocs/LaSalle Lab/Down Syndrome/DMRichR/sample_info_all.xlsx", sheet = "Males")
Males_Sample_Info_Clean <- Males_Sample_Info[1:32,]
Males <- glm(MajorHeartDefect ~ percent_cpg_meth, family = binomial(link=logit), data = Males_Sample_Info_Clean)
summary(Males)
Males <- glm(MajorHeartDefect ~ CD8T, family = binomial(link=logit), data = Males_Sample_Info_Clean)
summary(Males)
Males <- glm(MajorHeartDefect ~ CD4T, family = binomial(link=logit), data = Males_Sample_Info_Clean)
summary(Males)
